  {
    id: "0357",
    text: "{W80H1}{Widths 0,80,16,-1}{Speed 2}...{Speed -1}Farewell, {Zidane}{Speed 2}...{Speed -1}"
  },
  {
    id: "0358",
    text: "{W83H2}{Zidane}\n“Ah, dammit{Speed 2}...{Speed -1}”"
